<% form_for @owner_charge do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :owner_id %><br />
    <%= f.text_field :owner_id %>
  </p>
  <p>
    <%= f.label :charge_id %><br />
    <%= f.text_field :charge_id %>
  </p>
  <p>
    <%= f.label :for_year %><br />
    <%= f.text_field :for_year %>
  </p>
  <p>
    <%= f.label :description %><br />
    <%= f.text_field :description %>
  </p>
  <p><%= f.submit "Submit" %></p>
<% end %>
